pipeline {
    agent any 
    
    stages {

        stage ('Prepare Artifacts'){

            steps {
                sh '''
                zip -r ../frontend.zip *
                '''

            }
        }

        stage('Upload Artifacts'){

            steps {
                sh'''
                  curl -v -u admin:admin --upload-file frontend.zip http://3.216.95.216:8081/repository/frontendtodo/frontend.zip
                '''
            }
        }
    }
}